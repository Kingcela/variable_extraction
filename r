         </annotation>
         <annotation encoding="application/x-tex" id="p10.1.m16.1d">
          P_{n}
         </annotation>
        </semantics>
       </math>
       with
       <math alttext="1\leq n\leq\infty" class="ltx_Math" display="inline" id="p10.1.m17.1">
        <semantics id="p10.1.m17.1a">
         <mrow id="p10.1.m17.1.6" xref="p10.1.m17.1.6.cmml">
          <mn id="p10.1.m17.1.1" xref="p10.1.m17.1.1.cmml">
           1
          </mn>
          <mo id="p10.1.m17.1.2" xref="p10.1.m17.1.2.cmml">
           ‚â§
          </mo>
          <mi id="p10.1.m17.1.3" xref="p10.1.m17.1.3.cmml">
           n
          </mi>
          <mo id="p10.1.m17.1.4" xref="p10.1.m17.1.4.cmml">
           ‚â§
          </mo>
          <mi id="p10.1.m17.1.5" mathvariant="normal" xref="p10.1.m17.1.5.cmml">
           ‚àû
          </mi>
         </mrow>
         <annotation-xml encoding="MathML-Content" id="p10.1.m17.1b">
          <apply id="p10.1.m17.1.6.cmml" xref="p10.1.m17.1.6">
