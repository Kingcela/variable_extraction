          </annotation>
          <annotation encoding="application/x-tex" id="id3.p2.1.m2.1d">
           n\times n
          </annotation>
         </semantics>
        </math>
        system of matrix units for
        <math alttext="2\leq n&lt;\infty" class="ltx_Math" display="inline" id="id3.p2.1.m3.1">
         <semantics id="id3.p2.1.m3.1a">
          <mrow id="id3.p2.1.m3.1.6" xref="id3.p2.1.m3.1.6.cmml">
