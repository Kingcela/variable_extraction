          <annotation-xml encoding="MathML-Content" id="id3.p2.1.m2.1b">
           <apply id="id3.p2.1.m2.1.4.cmml" xref="id3.p2.1.m2.1.4">
            <times id="id3.p2.1.m2.1.2.cmml" xref="id3.p2.1.m2.1.2">
            </times>
            <ci id="id3.p2.1.m2.1.1.cmml" xref="id3.p2.1.m2.1.1">
             ùëõ
            </ci>
            <ci id="id3.p2.1.m2.1.3.cmml" xref="id3.p2.1.m2.1.3">
             ùëõ
            </ci>
           </apply>
          </annotation-xml>
