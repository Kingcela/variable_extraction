          </annotation>
          <annotation encoding="application/x-tex" id="id3.p2.1.m1.1d">
           P_{n}\mathcal{M}
          </annotation>
         </semantics>
        </math>
        contains an
        <math alttext="n\times n" class="ltx_Math" display="inline" id="id3.p2.1.m2.1">
         <semantics id="id3.p2.1.m2.1a">
          <mrow id="id3.p2.1.m2.1.4" xref="id3.p2.1.m2.1.4.cmml">
