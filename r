           P
          </mi>
          <mi id="p10.1.m16.1.2.1" xref="p10.1.m16.1.2.1.cmml">
           n
          </mi>
         </msub>
         <annotation-xml encoding="MathML-Content" id="p10.1.m16.1b">
          <apply id="p10.1.m16.1.3.cmml" xref="p10.1.m16.1.3">
           <csymbol cd="ambiguous" id="p10.1.m16.1.3.1.cmml" xref="p10.1.m16.1.3">
            subscript
           </csymbol>
           <ci id="p10.1.m16.1.1.cmml" xref="p10.1.m16.1.1">
            ùëÉ
           </ci>
           <ci id="p10.1.m16.1.2.1.cmml" xref="p10.1.m16.1.2.1">
            ùëõ
           </ci>
          </apply>
         </annotation-xml>
