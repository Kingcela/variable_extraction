            2
           </mn>
           <mo id="id3.p2.1.m3.1.2" xref="id3.p2.1.m3.1.2.cmml">
            ‚â§
           </mo>
           <mi id="id3.p2.1.m3.1.3" xref="id3.p2.1.m3.1.3.cmml">
            n
           </mi>
           <mo id="id3.p2.1.m3.1.4" xref="id3.p2.1.m3.1.4.cmml">
            &lt;
           </mo>
           <mi id="id3.p2.1.m3.1.5" mathvariant="normal" xref="id3.p2.1.m3.1.5.cmml">
            ‚àû
           </mi>
          </mrow>
          <annotation-xml encoding="MathML-Content" id="id3.p2.1.m3.1b">
           <apply id="id3.p2.1.m3.1.6.cmml" xref="id3.p2.1.m3.1.6">
            <and id="id3.p2.1.m3.1.6a.cmml" xref="id3.p2.1.m3.1.6">
            </and>
            <apply id="id3.p2.1.m3.1.6b.cmml" xref="id3.p2.1.m3.1.6">
             <leq id="id3.p2.1.m3.1.2.cmml" xref="id3.p2.1.m3.1.2">
             </leq>
             <cn id="id3.p2.1.m3.1.1.cmml" type="integer" xref="id3.p2.1.m3.1.1">
              2
             </cn>
             <ci id="id3.p2.1.m3.1.3.cmml" xref="id3.p2.1.m3.1.3">
              ùëõ
             </ci>
