           <mi class="ltx_font_mathcaligraphic" id="id2.p1.2.m5.1.3" xref="id2.p1.2.m5.1.3.cmml">
            ùí´
           </mi>
          </mrow>
          <annotation-xml encoding="MathML-Content" id="id2.p1.2.m5.1b">
