              ùëù
             </ci>
             <ci id="Thmlemma2.p1.1.1.m4.1.2.cmml" xref="Thmlemma2.p1.1.1.m4.1.2">
              normal-R
             </ci>
            </apply>
           </annotation-xml>
           <annotation encoding="application/x-llamapun" id="Thmlemma2.p1.1.1.m4.1c">
