             <mo id="id2.p1.2.m1.1.2" xref="id2.p1.2.m1.1.9.1.2.2.cmml">
              (
             </mo>
             <mrow id="id2.p1.2.m1.1.9.1.2.2" xref="id2.p1.2.m1.1.9.1.2.2.cmml">
              <mi id="id2.p1.2.m1.1.3" xref="id2.p1.2.m1.1.3.cmml">
               p
              </mi>
              <mo id="id2.p1.2.m1.1.9.1.2.2.1" xref="id2.p1.2.m1.1.9.1.2.2.1.cmml">
               ‚Å¢
