            I
           </mi>
           <mo id="p10.1.m14.1.4.2.1" xref="p10.1.m14.1.4.2.1.cmml">
            ‚Å¢
           </mo>
           <mi id="p10.1.m14.1.2" xref="p10.1.m14.1.2.cmml">
            I
           </mi>
          </mrow>
          <mo id="p10.1.m14.1.3" xref="p10.1.m14.1.4.2.cmml">
           ,
          </mo>
         </mrow>
         <annotation-xml encoding="MathML-Content" id="p10.1.m14.1b">
          <apply id="p10.1.m14.1.4.2.cmml" xref="p10.1.m14.1.4">
