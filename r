         </annotation>
         <annotation encoding="application/x-tex" id="p10.1.m14.1d">
          II,
         </annotation>
        </semantics>
       </math>
       and
       <math alttext="III" class="ltx_Math" display="inline" id="p10.1.m15.1">
        <semantics id="p10.1.m15.1a">
         <mrow id="p10.1.m15.1.4" xref="p10.1.m15.1.4.cmml">
          <mi id="p10.1.m15.1.1" xref="p10.1.m15.1.1.cmml">
